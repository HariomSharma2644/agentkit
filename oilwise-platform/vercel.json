{
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "cd web-dashboard && npm install && DISABLE_ESLINT_PLUGIN=true TSC_COMPILE_ON_ERROR=true npm run build",
  "outputDirectory": "web-dashboard/build",
  "installCommand": "cd web-dashboard && npm install",
  "env": {
    "NODE_ENV": "production",
    "REACT_APP_API_URL": "https://oilwise.vercel.app/api",
    "REACT_APP_AI_ENGINE_URL": "https://oilwise.vercel.app/ai",
    "REACT_APP_ENVIRONMENT": "production",
    "DISABLE_ESLINT_PLUGIN": "true",
    "TSC_COMPILE_ON_ERROR": "true"
  }
}

